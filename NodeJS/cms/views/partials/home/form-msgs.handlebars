{{#if error_messgae}}
    <div class="alert alert-danger">
        {{error_messgae}}
    </div>
{{/if}}

{{#if success_messgae}}
    <div class="alert alert-success">
        {{success_messgae}}
    </div>
{{/if}}

{{#if error}}
    <div class="alert alert-danger">
        {{error}}
    </div>
{{/if}}